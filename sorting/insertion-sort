#include <iostream>
#include <vector>
using namespace std;
void insertionSort(vector<int>& arr) {
    for (int i = 1; i < arr.size(); i++) {
        int copy = arr[i];
        int j = i - 1;

        while (j >= 0 && arr[j] > copy) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = copy;
    }
}

int main() {
    vector<int> data = {5,3,1,6,7,8,4};

    insertionSort(data);

    for (int num : data) {
        cout << num << " ";
    }
    cout << endl;

    return 0;
}
