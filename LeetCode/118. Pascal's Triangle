class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> nums(numRows);
     for(auto i = 0; i < numRows; i++){
        nums[i].resize(i + 1);
        nums[i][0] = nums[i][i] = 1;

        for(auto j = 1; j < i; j++){
                nums[i][j] = nums[i-1][j-1] + nums[i-1][j];
        }
     }
     
     return nums;   
    }
};
