class Solution {
public:
    vector<int> searchRange(vector<int>& arr, int target) {
        int last=-1,first =-1;
        for(int i=0;i<arr.size();i++)
        {
            if(arr[i] == target){
                first = i;
                break;
            }
        }
    for(int i=first;i<arr.size();i++){
        if(arr[i] == target){
                last = i;
            }
    }
        vector<int> res = {first,last};
        return res;
    }
};
